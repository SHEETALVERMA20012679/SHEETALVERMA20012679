#include<iostream>
using namespace std;

int main()
{
   int arr[100];
   int n;
   int key;
   int count =0;
   cout<<"no of element you want in array ";
   cin >> n;
   cout<<"no. of element in an array  :";

   for(int i=0;i<n;i++)
   {
  cin>>arr[i];
   }
   for(int i=0;i<n;i++)
{
 for(int j=0;j<n;j++)
    {
      if(i!=j)
      {
      for(int k=0;k<n;k++)
       {
          arr[k]=arr[i]+arr[j];
          cout<<arr[i]<<"+"<<arr[j]<<"+"<<arr[k]<<endl;
       }
     } 
}
}
}
